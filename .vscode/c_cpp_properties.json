{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../../include/**",
                "${workspaceFolder}/../../dep/include/**"
            ],
            "defines": [
                "ARCH_LIN=1"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++11",
            "intelliSenseMode": "linux-gcc-arm64"
        }
    ],
    "version": 4
}
